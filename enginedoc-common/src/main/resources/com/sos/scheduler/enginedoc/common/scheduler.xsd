	<xsd:element name="remote_scheduler.remote_task.kill">
		<xsd:complexType>
			<xsd:attribute name="process_id" type="xsd:long" use="required"/>
			<xsd:attribute name="signal">
				<xsd:simpleType>
					<xsd:restriction base="xsd:NMTOKEN">
