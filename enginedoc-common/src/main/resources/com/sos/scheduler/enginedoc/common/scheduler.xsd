													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="state" type="String" use="required"/>
									<xsd:attribute name="job" type="Path"/>
									<xsd:attribute name="next_state" type="String"/>
									<xsd:attribute name="error_state" type="String"/>
