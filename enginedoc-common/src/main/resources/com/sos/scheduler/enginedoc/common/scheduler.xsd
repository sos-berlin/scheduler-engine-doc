										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="suspend" type="Yes_no"/>
									<xsd:attribute name="delay" type="xsd:nonNegativeInteger"/>
									<xsd:anyAttribute namespace="##other" processContents="lax"/>
